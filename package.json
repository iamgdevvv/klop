{
	"name": "klop",
	"description": "Hackathon (IMPHNEN x Kolosal.ai)",
	"version": "1.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "npm run mantine::generate && npm run generate:importmap && npm run generate:types && eslint . && cross-env NODE_OPTIONS=--no-deprecation next build --webpack",
		"dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
		"devsafe": "rm -rf .next && npm run mantine::generate && npm run generate:importmap && npm run generate:types && cross-env NODE_OPTIONS=--no-deprecation next dev",
		"generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
		"generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
		"mantine::generate": "npx tailwind-preset-mantine ./src/lib/modules/theme-mantine.ts -o ./src/styles/mantine.css",
		"lint": "eslint .",
		"payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
		"start": "cross-env NODE_OPTIONS=--no-deprecation next start"
	},
	"dependencies": {
		"@mantine/core": "^8.3.9",
		"@mantine/form": "^8.3.9",
		"@mantine/hooks": "^8.3.9",
		"@mantine/nprogress": "^8.3.9",
		"@payloadcms/db-postgres": "^3.65.0",
		"@payloadcms/email-nodemailer": "^3.65.0",
		"@payloadcms/live-preview-react": "^3.65.0",
		"@payloadcms/next": "^3.65.0",
		"@payloadcms/plugin-seo": "^3.65.0",
		"@payloadcms/richtext-lexical": "^3.65.0",
		"@payloadcms/storage-s3": "^3.65.0",
		"@payloadcms/ui": "^3.65.0",
		"@tabler/icons-react": "^3.35.0",
		"@vercel/speed-insights": "^1.3.0",
		"clsx": "^2.1.1",
		"cross-env": "^10.1.0",
		"dayjs": "^1.11.19",
		"graphql": "^16.12.0",
		"lucide-react": "^0.555.0",
		"mantine-form-zod-resolver": "^1.3.0",
		"motion": "12.23.25",
		"next": "^16.0.6",
		"payload": "^3.65.0",
		"react": "19.2.0",
		"react-dom": "19.2.0",
		"sharp": "^0.34.5",
		"tailwind-merge": "^3.4.0",
		"tailwind-preset-mantine": "^3.0.3",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "4.1.17",
		"@types/node": "^24.10.1",
		"@types/react": "^19.2.7",
		"@types/react-dom": "^19.2.3",
		"autoprefixer": "^10.4.22",
		"babel-plugin-react-compiler": "^1.0.0",
		"critters": "^0.0.25",
		"eslint": "^9.39.1",
		"eslint-config-next": "16.0.6",
		"eslint-config-prettier": "^10.1.8",
		"postcss": "^8.5.6",
		"postcss-preset-mantine": "^1.18.0",
		"postcss-simple-vars": "^7.0.1",
		"prettier": "^3.7.3",
		"prettier-plugin-tailwindcss": "^0.7.1",
		"schema-dts": "^1.1.5",
		"tailwindcss": "4.1.17",
		"typescript": "^5.9.3"
	}
}
